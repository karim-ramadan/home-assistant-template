{
  "project_name": "Home Assistant Server",
  "repo_name": "{{ cookiecutter.project_name|lower|replace(' ', '_') }}",
  "domain_name": "homeassistant.yourdomain.com",
  "email": "you@example.com",
  "zigbee_usb_device": "/dev/ttyUSB0",
  "docker_network_name": "homeassistant_network",
  "default_container_restart_policy": "always",
  "duckdns_subdomain": "your_duckdns_subdomain",
